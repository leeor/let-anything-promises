{
  "name": "ppx-playground",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "__tests__",
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": ["@glennsl/bs-jest", "reason-promise"],
  "ppx-flags": ["bs-let/ppx"],
  "warnings": {
    "error": "+101"
  },
  "namespace": true,
  "refmt": 3
}
